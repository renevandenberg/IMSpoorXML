	<xs:element name="SwitchHeatingInstallations">
		<xs:annotation>
			<xs:documentation>Een verzameling Wisselverwarmingsinstallaties</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="SwitchHeatingInstallation" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="SwitchHeatingInstallation">
		<xs:annotation>
			<xs:documentation>Wisselverwarming is apparatuur waarmee de wisseltong van een wissel kan worden verwarmd.</xs:documentation>
			<xs:documentation xml:lang="NL">Wisselverwarminginstallatie</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointObject">
					<xs:sequence>
						<xs:element ref="SwitchHeatingCabinet" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
					<xs:attribute name="type" type="tHeatingTypeEnum" use="required">
						<xs:annotation>
							<xs:documentation>Het soort verwarming</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="SwitchHeatingCabinet">
		<xs:annotation>
			<xs:documentation>De wisselverwarmingskast bevat de bij wissels horende apparatuur waarmee de wisseltong, het beweegbare puntstuk of het beweegbare punt van het kruisstuk van het wissel van worden verwarmd.</xs:documentation>
			<xs:documentation xml:lang="NL">Wisselverwarmingskast</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointObject">
					<xs:attribute name="switchHeatingCabinetType" type="tSwitchHeatingCabinetTypeEnum" use="required">
						<xs:annotation>
							<xs:documentation>Het type wisselverwarmingskast. De lijst van opties is voorgedefiniëerd.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="TerrainLightingInstallations">
		<xs:annotation>
			<xs:documentation>Een verzameling terreinverlichtingsinstallaties</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="TerrainLightingInstallation" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="TerrainLightingInstallation">
		<xs:annotation>
			<xs:documentation>Terreinverlichting is het verlichtingssysteem dat bij paden en sporen wordt aangebracht ten
behoeve van de oriëntatie en zichtbaarheid van spoorobjecten bij afwezigheid van daglicht.</xs:documentation>
			<xs:documentation xml:lang="NL">Terreinverlichting</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointObject">
					<xs:sequence>
						<xs:element ref="LightMast" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="LightMast">
		<xs:annotation>
			<xs:documentation>Lichtmast</xs:documentation>
