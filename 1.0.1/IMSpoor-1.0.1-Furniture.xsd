			<xs:sequence>
				<xs:element ref="FireHydrant" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="FireHydrant">
		<xs:annotation>
			<xs:documentation>De bluswaterafnamehydrant is een tappunt op een onder druk staande waterleiding voor het
aansluiten van brandslangen om te voorzien in bluswater.</xs:documentation>
			<xs:documentation xml:lang="NL">Bluswaterafnamehydrant</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointObject"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="WeatherVanes">
