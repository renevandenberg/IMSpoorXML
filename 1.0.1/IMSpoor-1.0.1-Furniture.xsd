					<xs:attribute name="hasEscapeFunction" type="tTrueFalseEnum" use="required">
						<xs:annotation>
							<xs:documentation>Heeft de toegangspoort een vluchtfunctie.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="FireDepartmentKeyBox">
		<xs:annotation>
			<xs:documentation>Brandweersleutelkluis/noodsleutelkluis is een af te sluiten behuizing om een sleutel in op te bergen
t.b.v. hulpdiensten.</xs:documentation>
			<xs:documentation xml:lang="NL">Brandweersleutelkluis</xs:documentation>
		</xs:annotation>
