			<xs:complexContent>
				<xs:extension base="tLineObject">
					<xs:attribute name="height" type="xs:double">
						<xs:annotation>
							<xs:documentation>Hoogte t.o.v. Maaiveld in meters</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="fenceType" type="tFenceTypeEnum" use="required">
						<xs:annotation>
							<xs:documentation>Het type hekwerk.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
