			<xs:documentation>Een Servicepunt is een kast met sanitaire faciliteiten en aansluitingen voor elektriciteit.</xs:documentation>
			<xs:documentation xml:lang="NL">Servicepunt</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="tPointObject">
					<xs:attribute name="electricityConnection" type="tElectricityConnectionTypeEnum" use="required"/>
					<xs:attribute name="hasCompressedAirConnection" type="tTrueFalseEnum" use="required"/>
					<xs:attribute name="waterConnection" type="tWaterConnectionTypeEnum" use="required"/>
					<xs:attribute name="hasWashFillHydrant" type="tTrueFalseEnum" use="required"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="Culverts">
		<xs:annotation>
			<xs:documentation>Een verzameling duikers</xs:documentation>
		</xs:annotation>
