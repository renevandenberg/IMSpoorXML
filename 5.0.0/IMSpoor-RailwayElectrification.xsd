				<xs:extension base="tWirePartInfo"/>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="tOverheadLineConnectionPoint">
		<xs:annotation>
			<xs:documentation>Een Elektrische-Verbinding-BVL</xs:documentation>
			<xs:documentation xml:lang="NL">Elektrische-Verbinding-BVL</xs:documentation>
		</xs:annotation>
