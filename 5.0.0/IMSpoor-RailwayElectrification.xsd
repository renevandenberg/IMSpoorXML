			<xs:complexContent>
				<xs:extension base="tPointObject">
					<xs:sequence>
						<xs:element ref="WirePartShiftingInfo" maxOccurs="unbounded"/>
						<xs:element name="SeparatorRefs" type="tUUIDRefList">
							<xs:annotation>
								<xs:documentation>Een lijst met PUIC verwijzingen naar een of meerdere richtconstructiescheiders.</xs:documentation>
								<xs:appinfo>
									<ObjectTypeRef>Insulator</ObjectTypeRef>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
