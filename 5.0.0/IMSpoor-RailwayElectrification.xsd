			<xs:complexContent>
				<xs:extension base="tPointObject">
					<xs:sequence>
						<xs:element name="ContactWireBraceConnectionPointRefs" type="tUUIDRefList" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Een lijst met PUIC verwijzingen naar een of meerdere richtconstructie aansluitpunten.</xs:documentation>
								<xs:appinfo>
									<ObjectTypeRef>ContactWireBraceConnectionPoint</ObjectTypeRef>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
						<xs:element name="ContactWireBraceRefs" type="tUUIDRefList" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Een lijst met PUIC verwijzingen naar een of meerdere andere richtconstructies waarmee deze richtconstructie elektrisch is verbonden.</xs:documentation>
								<xs:appinfo>
									<ObjectTypeRef>ContactWireBrace</ObjectTypeRef>
								</xs:appinfo>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="ContactWireBraceConnectionPoints">
		<xs:annotation>
			<xs:documentation>Een verzameling richtconstructie aansluitpunten</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ContactWireBraceConnectionPoint" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ContactWireBraceConnectionPoint">
		<xs:annotation>
			<xs:documentation>Dit is een aansluitpunt van een richtconstructie.</xs:documentation>
			<xs:documentation xml:lang="NL">Richtconstructie-Aansluitpunt</xs:documentation>
		</xs:annotation>
